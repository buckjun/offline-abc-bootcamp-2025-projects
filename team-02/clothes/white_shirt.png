  <ellipse class="shirt" cx="165" cy="80" rx="15" ry="25"/>
  
  <!-- 칼라 -->
  <path class="collar" d="M70 50 L100 40 L130 50 L120 65 L100 60 L80 65 Z"/>
  
  <!-- 버튼들 -->
  <circle class="button" cx="100" cy="80" r="3"/>
  <circle class="button" cx="100" cy="100" r="3"/>
  <circle class="button" cx="100" cy="120" r="3"/>
  <circle class="button" cx="100" cy="140" r="3"/>
  <circle class="button" cx="100" cy="160" r="3"/>
  
  <!-- 주머니 -->
  <rect class="collar" x="70" y="100" width="20" height="15" rx="2"/>
</svg>
